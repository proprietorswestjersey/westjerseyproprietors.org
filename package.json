{
  "name": "westjerseyproprietors.org",
  "description": "West Jersey Proprietors Website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "eleventy",
    "fix": "prettier --cache --write '*.{md,js}' 'src/**/*'",
    "start": "eleventy --incremental --serve",
    "dev": "npm run start",
    "test": "prettier --cache --check '*.{md,js}' 'src/**/*'"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-vite": "^3.0.0",
    "@shopify/prettier-plugin-liquid": "^1.9.3",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.0",
    "vite": "^5.0.8"
  },
  "prettier": {
    "plugins": [
      "@shopify/prettier-plugin-liquid"
    ]
  }
}
